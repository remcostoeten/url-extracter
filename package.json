{
  "name": "url-extractor",
  "version": "0.1.0",
  "private": true,
  "author": "remco stoeten",
  "description": "Extracts urls from text and some other magic.",
  "keywords": [
    "Next.js",
    "React",
    "Tailwind CSS",
    "Radix UI",
    "shadcn/ui"
  ],
  "homepage": "https://extract.remcostoeten.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/remcostoeten/url-extracter.git"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev0": "next dev --turbo --port 0000",
    "build": "next build",
    "buildfull": "prettier --write \"**/*.{*tail*.js*,ts,tsx,mdx,css}\" --cache && eslint --fix . && next build",
    "start": "next start",
    "lint:fix": "eslint --fix . --config ./.eslintrc.json",
    "eslintfix": "eslint --fix . --config ./.eslintrc.json",
    "eslint:check": "eslint .",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx,css}\" --cache",
    "format:check": "prettier --check \"**/*.{json,ts,tsx,mdx}\" --cache",
    "check": "npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tabler/icons-react": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.1.7",
    "lucide-react": "^0.330.0",
    "million": "^3.0.6",
    "next": "14.1.0",
    "next-themes": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}